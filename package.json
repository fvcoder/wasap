{
  "name": "w",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev-core": "run-p dev-core-*",
    "dev-core-core": "cd packages/core && pnpm run dev",
    "dev-core-app": "cd apps/example && pnpm run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-codely": "^2.1.3",
    "npm-run-all": "^4.1.5",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.28.0"
}